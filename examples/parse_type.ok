
// Does &&void parse correctly?
fn ref_one(ptr: &void) -> &&void {
    return &ptr;
}

// Does &&&void parse correctly?
fn ref_two(ptr: &&void) -> &&&void {
    return &ptr;
}

// Does &&&&void parse correctly?
fn ref_three(ptr: &&&void) -> &&&&void {
    return &ptr;
}

fn main() {
    let p: &void = alloc(32);
    let _ = ref_three(ref_two(ref_one(p)));
    free p: 32;
}