#[std]


fn main() {
    let size = 10;
    // without `&num` type specification, oak will throw a
    // `cannot use type inference when defining void pointer` error.
    let x = alloc(size) as &num;

    for i in 0..size { x[i] = i * 5; }

    for i in 0..size { putnumln(x[i]); }

    free x: size;
}